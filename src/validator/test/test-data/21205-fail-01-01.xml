<?xml version="1.0" encoding="UTF-8"?>

<!--
Fail: (21205) The order of subelements within Event must be the
following: trigger, delay, listOfEventAssignments. The delay element
is optional, but if present, must follow trigger. (References: L2V2
Section 4.14.)
-->

<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model>

    <listOfCompartments>
      <compartment id="c" constant="false"/>
    </listOfCompartments>

    <listOfEvents>
      <event id="e">

        <delay>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <cn> 1 </cn>
          </math>
        </delay>

        <trigger>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply> <neq/> <cn> 0 </cn> <cn> 1 </cn> </apply>
          </math>
        </trigger>

        <listOfEventAssignments>
          <eventAssignment variable="c">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <cn> 0 </cn>
            </math>
          </eventAssignment>
        </listOfEventAssignments>

      </event>
    </listOfEvents>

  </model>
</sbml>
