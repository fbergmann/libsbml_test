    <listOfCompartments>
      <compartment id="c" units="litre" constant="false"/>
    </listOfCompartments>
    <listOfParameters>
      <parameter id="p" value="1" units="ampere" constant="false"/>
    </listOfParameters>
    <listOfRules>
      <rateRule variable="p">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> c </ci>
            <cn> 2 </cn>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
  </model>
