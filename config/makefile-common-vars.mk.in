# -----------------------------------------------------------------------------
# Commmon variables.
# -----------------------------------------------------------------------------
#
# For '@' variables (which are expanded by configure), make sure not to
# put anything in here that might expand to a relative path.
#
# `prefix', `exec_prefix', `top_srcdir' and `top_builddir' must be in
# lowercase, because some of the autoconf macros expand into strings that
# use them.
#
# The enclosing makefile is assumed to define `srcdir'.

prefix            = @prefix@
exec_prefix       = @exec_prefix@

top_srcdir        = @abs_top_srcdir@
top_builddir      = @abs_top_builddir@
TOP_SRCDIR        = $(top_srcdir)
TOP_BUILDDIR      = $(top_builddir)
DISTDIR           = $(top_srcdir)/$(PACKAGE_TARNAME)-$(PACKAGE_VERSION)
VPATH             = $(srcdir)

BINDIR            = @bindir@
DATADIR           = @datadir@
DOCDIR            = $(DATADIR)/doc/$(PACKAGE_TARNAME)-$(PACKAGE_VERSION)
INCLUDEDIR        = @includedir@
LIBDIR            = @libdir@
MANDIR            = @mandir@

USE_LIBCHECK      = @USE_LIBCHECK@
LIBCHECK_CPPFLAGS = @LIBCHECK_CPPFLAGS@
LIBCHECK_LDFLAGS  = @LIBCHECK_LDFLAGS@
LIBCHECK_LIBS     = @LIBCHECK_LIBS@

USE_EXPAT         = @USE_EXPAT@
EXPAT_CPPFLAGS    = @EXPAT_CPPFLAGS@
EXPAT_LDFLAGS     = @EXPAT_LDFLAGS@
EXPAT_LIBS        = @EXPAT_LIBS@

USE_XERCES        = @USE_XERCES@
XERCES_CPPFLAGS   = @XERCES_CPPFLAGS@
XERCES_LDFLAGS    = @XERCES_LDFLAGS@
XERCES_LIBS       = @XERCES_LIBS@

USE_PYTHON        = @USE_PYTHON@
PYTHON            = @PYTHON@
PYTHON_CPPFLAGS   = @PYTHON_CPPFLAGS@
PYTHON_LDFLAGS    = @PYTHON_LDFLAGS@
PYTHON_LIBS       = @PYTHON_LIBS@

USE_JAVA          = @USE_JAVA@
JAVA              = @JAVA@
JAVAC             = @JAVAC@
JAR               = @JAR@
JAVA_CPPFLAGS     = @JAVA_CPPFLAGS@
JAVA_LDFLAGS      = @JAVA_LDFLAGS@
JAVA_LIBS         = @JAVA_LIBS@
JNIEXT            = @JNIEXT@

USE_MATLAB        = @USE_MATLAB@
MATLAB            = @MATLAB@
MATLAB_CPPFLAGS   = @MATLAB_CPPFLAGS@
MATLAB_LDFLAGS    = @MATLAB_LDFLAGS@
MATLAB_LIBS       = @MATLAB_LIBS@

USE_SWIG          = @USE_SWIG@
SWIG              = @SWIG@
SWIGLIB           = @SWIGLIB@
SWIGFLAGS         = @SWIGFLAGS@

ACLOCAL           = @ACLOCAL@
ACLOCAL_FLAGS     = --acdir=$(top_srcdir)/config
ACLOCAL_M4        = $(top_srcdir)/aclocal.m4
AUTOCONF          = @AUTOCONF@

AR                = @AR@
BUILD             = @build@
BUILD_CPU         = @build_cpu@
BUILD_OS          = @build_os@
CC                = @CC@
CD                = CDPATH="$${ZSH_VERSION+.}$(PATH_SEPARATOR)" && cd
CFLAGS            = @CFLAGS@
CPP               = @CPP@
CPPFLAGS          = @CPPFLAGS@
CTAGS             = ctags
CTAGSFLAGS        =
CXX               = @CXX@
CXXFLAGS          = @CXXFLAGS@
CYGPATH_W         = @CYGPATH_W@
DEFS              = @DEFS@
DEPDIR            = .deps
DEPEXT            = Po
ETAGS             = etags
ETAGSFLAGS        =
EXEEXT            = @EXEEXT@
HOST_OS           = @host_os@
HOST_TYPE         = @HOST_TYPE@
INSTALL           = @INSTALL@
INSTALL_SH        = $(top_srcdir)/config/install-sh
LDFLAGS           = @LDFLAGS@
LIBS              = @LIBS@
MKINSTALLDIRS     = $(SHELL) $(top_srcdir)/config/mkinstalldirs
OBJEXT            = @OBJEXT@
PACKAGE           = @PACKAGE_TARNAME@
PACKAGE_BUGREPORT = @PACKAGE_BUGREPORT@
PACKAGE_NAME      = @PACKAGE_NAME@
PACKAGE_STRING    = @PACKAGE_STRING@
PACKAGE_TARNAME   = @PACKAGE_TARNAME@
PACKAGE_VERSION   = @PACKAGE_VERSION@
RANLIB            = @RANLIB@
SHAREDLIBEXT      = @SHAREDLIBEXT@
SHELL             = @SHELL@
TAR               = @TAR@


