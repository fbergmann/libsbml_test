CLASSPATH = ../../src/bindings/java/libsbmlj.jar:.

JAVA   = @JAVA@
JAVAC  = @JAVAC@
JFLAGS = -classpath $(CLASSPATH)

classes = convertSBML.class printMath.class printSBML.class readSBML.class \
          translateMath.class validateSBML.class evaluateMath.class


all: $(classes)

convertSBML.class: convertSBML.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^

evaluateMath.class: evaluateMath.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^; \
  if test $$? -ne 0; \
    then echo "*** Sorry JDK 1.5 is required to compile evaluateMath ***"; fi

printMath.class: printMath.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^

printSBML.class: printSBML.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^

readSBML.class: readSBML.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^

translateMath.class: translateMath.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^

validateSBML.class: validateSBML.java libSBMLHelper.java
	$(JAVAC) $(JFLAGS) $^

clean:
	/bin/rm -f $(classes) *~

distclean: clean
	/bin/rm Makefile


# The following are generic 'make' targets that are not used in
# this simple examples directory.

check dist docs install install-docs uninstall \
mostlyclean maintainer-clean installcheck dvi pdf ps info html \
distcheck:
